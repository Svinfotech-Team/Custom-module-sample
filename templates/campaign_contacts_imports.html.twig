<table class="table table-striped">
  <thead>
  <tr>
    <th scope="col">Email</th>
    <th scope="col">User ID</th>
    <th scope="col">Action</th>
  </tr>
  </thead>
  <tbody>
  {% for user in content.users %}
    <tr>
      <td>{{ user.email }}</td>
      <td>{{ user.id }}</td>
      <td>
        <a href="{{ content.base }}/admin/config/active-campaign/contact/imports/user/{{ user.id }}">Export</a>
      </td>
    </tr>
  {% endfor %}
  </tbody>
</table>